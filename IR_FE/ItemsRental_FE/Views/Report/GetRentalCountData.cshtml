@model IEnumerable<ItemsRental_FE.Models.ItemRentalCountReport>

@{
    ViewBag.Title = "ItemsRentalReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetRentalCountData</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RentalCount)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RentalCount)
            </td>

        </tr>
    }

</table>

<div>
    <h2>Chart</h2>
    <img src="@Url.Action("DrawItemRentalCountChart")" />
</div>
